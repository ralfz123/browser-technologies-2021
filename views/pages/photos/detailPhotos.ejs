<head>
  <%- include ('../../partials/head') %>
  <title>Photos #1 | iPic</title>
</head>
<body>
  <%- include ('../../partials/header') %>
  <main>
    <section>
      <a href="/photos">Back</a>
      <form action="/photoDelete" method="POST">
        <fieldset>
          <legend>Info photo</legend>

          <h1>Photo #<%= photoData.id%></h1>
          <p><%= photoData.title%></p>
          <p><%= photoData.description%></p>
          <p><%= photoData.photographer%></p>
          <p><%= photoData.location%></p>

          <button type="submit">Delete photo</button>
          <button type="button">Edit</button>
        </fieldset>
      </form>
    </section>
  </main>

  <%- include ('../../partials/footer') %>
</body>
